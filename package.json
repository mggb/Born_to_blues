{
  "name": "born-to-blues",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "connected-react-router": "^5.0.1",
    "history": "^4.7.2",
    "i18next": "^12.0.0",
    "ramda": "^0.26.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-i18next": "^8.3.8",
    "react-newline-to-break": "^1.0.6",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.1.3",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.23.1",
    "video-react": "^0.13.2"
  },
  "scripts": {
    "flow": "flow",
    "lint": "eslint 'src/*.{js,jsx}' --quiet",
    "prettier": "prettier --single-quote --write \"src/**/*.js\"",
    "start": "react-scripts start",
    "start:server": "cd server && adonis serve --dev",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage": "react-scripts test --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx}",
      "src/utils/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "flow-bin": "^0.86.0",
    "prettier": "^1.15.2",
    "react-test-renderer": "^16.6.3"
  }
}
