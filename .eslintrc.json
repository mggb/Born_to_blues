{
    "extends": [
        "react-app",
        "plugin:jsx-a11y/recommended",
        "airbnb",
        "prettier",
        "prettier/flowtype",
        "prettier/react"
    ],
    "plugins": ["jsx-a11y"],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
      }
}